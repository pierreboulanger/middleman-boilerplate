<div id="team" class="text-center">
  <h1 class="title">Our Team</h1>

  <ul class="list-inline">
    <% ["anne", "seb", "romain"].each do |member| %>
      <li>
        <%= image_tag "#{member}.png", class: "img-circle avatar-xlarge" %>
        <h3><%= member.capitalize %></h3>
      </li>
    <% end %>
  </ul>

  <h3 class="padded"><a href="mailto:contact@airbnb.com"><i class="fa fa-envelope"></i>contact@airbnb.com</a></h3>
</div>
